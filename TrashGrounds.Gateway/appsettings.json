{
  "Kestrel": {
    "Endpoints": {
      "Https": {
        "Url": "https://*:8000",
        "Certificate": {
          "Path": "/app/Certificates/cert.pem",
          "KeyPath": "/app/Certificates/key.pem"
        }
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
